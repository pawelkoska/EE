<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/status/categoryCreateSuccess.xhtml</to-view-id>
            <redirect />
        </navigation-case>   
             
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/faces/index.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
               
        <navigation-case>
            <from-outcome>createCategory</from-outcome>
            <to-view-id>/faces/category/createCategory.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        
        <navigation-case>
            <from-outcome>createExamination</from-outcome>
            <to-view-id>/faces/examination/createExamination.xhtml</to-view-id>
            <redirect />
        </navigation-case>
                
        <navigation-case>
            <from-outcome>examinationSuccess</from-outcome>
            <to-view-id>/faces/status/examinationCreateSuccess.xhtml</to-view-id>
            <redirect />
        </navigation-case>   
         
        <navigation-case>
            <from-outcome>ourOffer</from-outcome>
            <to-view-id>/faces/examination/ourOffer.xhtml</to-view-id>
            <redirect />
        </navigation-case>   
         
        <navigation-case>
            <from-outcome>allCategories</from-outcome>
            <to-view-id>/faces/category/allCategories.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
           
        <navigation-case>
            <from-outcome>allExaminations</from-outcome>
            <to-view-id>/faces/examination/allExaminations.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
           
        <navigation-case>
            <from-outcome>editCategory</from-outcome>
            <to-view-id>/faces/category/editCategory.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
        
        <navigation-case>
            <from-outcome>editSuccess</from-outcome>
            <to-view-id>/faces/category/editCategory.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
                                
    </navigation-rule>
</faces-config>
