<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <navigation-case>
            <from-outcome>createCategorySuccess</from-outcome>
            <to-view-id>/status/categoryCreateSuccess.xhtml</to-view-id>
            <redirect />
        </navigation-case>   
             
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/home.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
        
        <navigation-case>
            <from-outcome>logOut</from-outcome>
            <to-view-id>/home.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
               
        <navigation-case>
            <from-outcome>createCategory</from-outcome>
            <to-view-id>/category/createCategory.xhtml</to-view-id>
            <redirect />
        </navigation-case>
        
        <navigation-case>
            <from-outcome>createExamination</from-outcome>
            <to-view-id>/examination/createExamination.xhtml</to-view-id>
            <redirect />
        </navigation-case>
                
        <navigation-case>
            <from-outcome>examinationSuccess</from-outcome>
            <to-view-id>/status/examinationCreateSuccess.xhtml</to-view-id>
            <redirect />
        </navigation-case>   
         
        <navigation-case>
            <from-outcome>ourOffer</from-outcome>
            <to-view-id>/examination/ourOffer.xhtml</to-view-id>
            <redirect />
        </navigation-case>   
         
        <navigation-case>
            <from-outcome>allCategories</from-outcome>
            <to-view-id>/category/allCategories.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
           
        <navigation-case>
            <from-outcome>allExaminations</from-outcome>
            <to-view-id>/examination/allExaminations.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
           
        <navigation-case>
            <from-outcome>editCategory</from-outcome>
            <to-view-id>/category/editCategory.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
        
        <navigation-case>
            <from-outcome>editSuccess</from-outcome>
            <to-view-id>/category/editCategory.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
        
        <navigation-case>
            <from-outcome>deleteCategorySuccess</from-outcome>
            <to-view-id>/status/deleteCategorySuccess.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
        
        <navigation-case>
            <from-outcome>deleteExaminationSuccess</from-outcome>
            <to-view-id>/status/deleteExaminationSuccess.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
        
        <navigation-case>
            <from-outcome>editExamination</from-outcome>
            <to-view-id>/examination/editExamination.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
        
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login/loginForm.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
        
        <navigation-case>
            <from-outcome>createAppointment</from-outcome>
            <to-view-id>/appointment/createAppointment.xhtml</to-view-id>
            <redirect />
        </navigation-case> 
       
                                
    </navigation-rule>
</faces-config>
